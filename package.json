{
  "name": "charkchalk-frontend",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "ng serve --hmr",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:cli": "yarn test --no-progress --code-coverage --browsers=ChromeHeadless",
    "lint": "ng lint",
    "format": "eslint --fix . && prettier --write --ignore-unknown .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./**/*.{ts,html}": "eslint --cache --fix",
    "./**/*.**": "prettier --check --ignore-unknown"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.3",
    "@angular/common": "^16.0.3",
    "@angular/compiler": "^16.0.3",
    "@angular/core": "^16.0.3",
    "@angular/forms": "^16.0.3",
    "@angular/platform-browser": "^16.0.3",
    "@angular/platform-browser-dynamic": "^16.0.3",
    "@angular/router": "^16.0.3",
    "@angular/service-worker": "^16.0.3",
    "ng-http-caching": "^16.0.4",
    "object-hash": "^3.0.0",
    "primeflex": "^3.3.0",
    "primeicons": "^6.0.1",
    "primeng": "16.0.0-rc.2",
    "rxjs": "~7.8.1",
    "tslib": "^2.5.2",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.3",
    "@angular-eslint/builder": "16.0.2",
    "@angular-eslint/eslint-plugin": "16.0.2",
    "@angular-eslint/eslint-plugin-template": "16.0.2",
    "@angular-eslint/schematics": "16.0.2",
    "@angular-eslint/template-parser": "16.0.2",
    "@angular/cli": "~16.0.3",
    "@angular/compiler-cli": "^16.0.3",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@ngx-env/builder": "16.0.1",
    "@types/jasmine": "~4.3.2",
    "@types/node": "^20.2.5",
    "@types/object-hash": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@typescript-eslint/parser": "5.59.7",
    "angular-cli-ghpages": "1.0.6",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "jasmine-core": "~5.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prettier-eslint": "^15.0.1",
    "typescript": "~5.0.4"
  }
}
